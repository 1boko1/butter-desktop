/*    Montserrat    */

@font-face
    font-family: 'Montserrat'
    font-weight: normal
    font-style: normal
    src: url(../fonts/Montserrat/woff/Montserrat-Regular.woff)

@font-face
    font-family: 'Montserrat Semibold'
    font-weight: 600
    font-style: normal
    src: url(../fonts/Montserrat/woff/Montserrat-Semibold.woff)

@font-face
    font-family: 'Montserrat Bold'
    font-weight: bold
    font-style: normal
    src: url(../fonts/Montserrat/woff/Montserrat-Bold.woff)
/*    Noto Sans    */

@font-face
    font-family: 'Noto Sans'
    font-weight: normal
    font-style: normal
    src: url(../fonts/Noto-sans/woff/NotoSans-Regular.woff)

@font-face
    font-family: 'Noto Sans Bold'
    font-weight: normal
    font-style: normal
    src: url(../fonts/Noto-sans/woff/NotoSans-Bold.woff)

/*    Material Icons    */

@font-face
    font-family: 'Material Icons'
    font-style: normal
    font-weight: 400
    src: local('Material Icons'),
         local('MaterialIcons-Regular'),
         url(../fonts/Material-icons/MaterialIcons-Regular.woff2) format('woff2'),
         url(../fonts/Material-icons/MaterialIcons-Regular.woff) format('woff')

/*    VideoJS    */

@font-face
    font-family: 'VideoJS'
    font-weight: normal
    font-style: normal
    src: url('../fonts/Vjs/vjs.eot')
    src: url('../fonts/Vjs/vjs.eot?#iefix') format('embedded-opentype'),
         url('../fonts/Vjs/vjs.woff') format('woff'),
         url('../fonts/Vjs/vjs.ttf') format('truetype')
